#include <fstream>
#include <iostream>
#include <string>

using namespace std;

void f1() {
    fstream G;
    ifstream file(R"(C:\Users\denis\CLionProjects\untitled\F.txt)");
    G.open(R"(C:\Users\denis\CLionProjects\untitled\G.txt)", ios::app);
    string str;
    while (getline(file, str)) {
        for (int i = str.size() - 1; i >= 0; --i) {
            if (str[i] == '1')
                str[i] = '0';
            else if (str[i] == '0')
                str[i] = '1';
            G << str[i];
        }
        G << endl;
    }
}

int main() {
    f1();
}
