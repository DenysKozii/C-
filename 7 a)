#include <iostream>
#include <stdio.h>

#include <cmath>
using namespace std;

void printSquareEquation(double a, double b, double c){
    if(a == 0){
        cout << "x = ";
        cout << b == 0 ? 0 : c/b;
        return;
    }
    double D = b*b - 4*a*c;
    if (D < 0){
        cout << "no x";
        return;
    }
    double x1 = (-b + sqrt(D))/(2*a);
    double x2 = (-b - sqrt(D))/(2*a);
    cout << "x1 = " << x1 ;
    if (D != 0){
        cout << " x2 = " << x2;
    }
}



double a,b,c;

int main()
{
    cin >> a >> b >> c;
    printSquareEquation(a , b, c);
    return 0;
}
