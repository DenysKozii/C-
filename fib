#include <iostream>

using namespace std;

unsigned a, res;

unsigned fib(unsigned  a){
    if(a==1 || a==0) {
        return a;
    }
    else {
        if (fib(a - 1) + fib(a - 2) < a) {
            res = fib(a - 1) + fib(a - 2);
            return fib(a - 1) + fib(a - 2);
        } else {
            return res;
        }
    }
}



int main() {
    cin >> a;
    cout << fib(a) << endl;
    return 0;
}
