#include <iostream>

using namespace std;
int N;

int* sorting(int arr[]) {
    int max;
    int index;
    for (int i = N-1; i >= 0; i--){
        max = arr[i];
        index = i;
        for (int j = 0; j <= i; j++){
            if(arr[j] > max){
                max = arr[j];
                index = j;
            }
        }
        arr[index] = arr[i];
        arr[i] = max;
    }
    return arr;
}

float f(int arr[]) {
    arr = sorting(arr);
    for (int i = 0 ; i < N; i++)
        cout << arr[i] << endl;
    return N % 2 == 1 ? float(arr[N/2]) : (float(arr[N/2]) + float(arr[N/2-1]))/2;
}

int main() {
    cout << "N=";
    cin >> N;
    int arr[N];
    for (int i = 0; i < N; ++i) {
        cout << "arr[" << i << "]=";
        cin >> arr[i];
    }
    cout << f(arr);
}
